{
  "name": "@mmit/vue-ts-starter",
  "version": "0.1.13",
  "private": true,
  "description": "VUE Starter-Template",
  "author": "Mike Mitterer",
  "scripts": {
    "build": "vue-cli-service build --mode development",
    "test:unit": "vue-cli-service test:unit tests/unit/.*",
    "lint": "vue-cli-service lint",
    "deploy": "yarn version --new-version patch",
    "build:prod": "vue-cli-service build --mode production",
    "clean": "rm -rf lib dist bin && jest --clearCache",
    "start": "vue-cli-service serve --mode development --open",
    "start:prod": "vue-cli-service serve --mode production --open",
    "tc": "tsc --noEmit",
    "test": "yarn run test:unit",
    "test:integration": "vue-cli-service test:unit --runInBand --detectOpenHandles tests/integration/.*"
  },
  "dependencies": {
    "@mmit/logging": "^0.1.5",
    "@mmit/styles": "^0.1.21",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.5",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "27.0.2",
    "@types/workbox-window": "^4.3.4",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.11",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/test-utils": "1.2.2",
    "babel-jest": "27.2.5",
    "babel-loader": "^8.2.2",
    "jest": "27.2.5",
    "moment": "^2.29.1",
    "sass": "~1.32",
    "sass-loader": "10",
    "ts-jest": "27.0.5",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "^4.4.4",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.12",
    "vuex-module-decorators": "^1.0.1",
    "workbox-webpack-plugin": "^6.3.0"
  },
  "keywords": [
    "typescript vue"
  ],
  "resolutions": {
    "@vue/cli-plugin-unit-jest/jest": "24",
    "@vue/cli-plugin-unit-jest/babel-jest": "24"
  }
}
