{
    "name": "@mmit/vuetsstarter",
    "version": "0.1.12",
    "author": "Mike Mitterer",
    "description": "VUE Starter-Template",
    "keywords": [
        "typescript vue"
    ],
    "private": true,
    "scripts": {
        "start": "vue-cli-service serve --mode development --open",
        "tc": "tsc --noEmit",
        "lint": "vue-cli-service lint",
        "test": "yarn run test:unit",
        "test:unit": "vue-cli-service test:unit tests/unit/.*",
        "test:integration": "vue-cli-service test:unit --runInBand --detectOpenHandles tests/integration/.*",
        "build": "vue-cli-service build --mode development",
        "build:prod": "vue-cli-service build --mode production",
        "deploy": "yarn version --new-version patch"
    },
    "dependencies": {
        "@mmit/logging": "^0.1.5",
        "vue": "^2.6.11",
        "vue-class-component": "^7.2.5",
        "vue-property-decorator": "^9.0.0",
        "vue-router": "^3.3.4",
        "vuex": "^3.5.1"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@types/jest": "26",
        "@vue/cli-plugin-typescript": "^4.4.6",
        "@vue/cli-plugin-unit-jest": "^4.4.6",
        "@vue/cli-service": "^4.4.6",
        "@vue/test-utils": "1.0.3",
        "babel-jest": "26",
        "jest": "26",
        "moment": "^2.27.0",
        "node-sass": "^4.14.1",
        "sass-loader": "^9.0.2",
        "ts-jest": "26",
        "tslint-consistent-codestyle": "^1.16.0",
        "typescript": "^3.9.7",
        "vue-jest": "^4.0.0-beta.2",
        "vue-template-compiler": "^2.6.11",
        "vuex-module-decorators": "^0.17.0"
    },
    "resolutions": {
        "@vue/cli-plugin-unit-jest/jest": "24",
        "@vue/cli-plugin-unit-jest/babel-jest": "24"
    }
}
