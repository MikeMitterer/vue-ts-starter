{
    "name": "@mmit/vuetsstarter",
    "version": "0.1.4",
    "author": "Mike Mitterer",
    "description": "VUE Starter-Template",
    "keywords": [
        "typescript vue"
    ],
    "private": true,
    "scripts": {
        "start": "vue-cli-service serve --mode development --open",
        "build": "vue-cli-service build --mode development",
        "build:prod": "vue-cli-service build --mode production",
        "lint": "vue-cli-service lint",
        "deploy": "yarn run build:prod && yarn version --new-version patch",
        "test": "yarn run test:unit",
        "test:unit": "vue-cli-service test:unit"
    },
    "dependencies": {
        "moment": "^2.24.0",
        "vue": "^2.6.10",
        "vue-class-component": "^7.0.2",
        "vue-property-decorator": "^8.1.0",
        "vue-router": "^3.0.6",
        "vuex": "^3.1.0"
    },
    "devDependencies": {
        "@types/jest": "^24.0.12",
        "@vue/cli-plugin-typescript": "^3.7.0",
        "@vue/cli-plugin-unit-jest": "^3.7.0",
        "@vue/cli-service": "^3.7.0",
        "@vue/test-utils": "1.0.0-beta.29",
        "node-sass": "^4.12.0",
        "sass-loader": "^7.1.0",
        "ts-jest": "^24.0.2",
        "typescript": "^3.4.5",
        "vue-template-compiler": "^2.6.10",
        "vuex-module-decorators": "^0.9.8"
    }
}
