{
  "name": "@mmit/vue-ts-starter",
  "version": "0.1.14",
  "private": true,
  "description": "VUE Starter-Template",
  "author": "Mike Mitterer",
  "scripts": {
    "pkg:version": "echo $(jq -r -j '.name' < package.json;echo -n ': ';jq -r -j '.version' < package.json)",
    "clean": "rm -rf lib dist bin && jest --clearCache",
    "update:browser": "browserslist --update-db",
    "download:certs": "rm -rf .ssl && cert2ssl.sh -d int.mikemitterer.at",
    "build": "vue-cli-service build --mode development",
    "test:unit": "jest --colors test/unit",
    "lint": "tslint --project tsconfig.json",
    "deploy": "yarn version --new-version patch",
    "build:prod": "vue-cli-service build --mode production",
    "start": "vue-cli-service serve --mode development --open",
    "start:prod": "vue-cli-service serve --mode production --open",
    "tc": "tsc --noEmit",
    "test": "yarn run test:unit",
    "test:integration": "vue-cli-service test:unit --runInBand --detectOpenHandles tests/integration/.*"
  },
  "keywords": [
    "typescript vue"
  ],
  "dependencies": {
    "@mmit/logging": "0.1.8",
    "@mmit/styles": "^0.1.21",
    "register-service-worker": "^1.7.1",
    "vue": "2.7",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@mdi/font": "^5.6.55",
    "@types/google.visualization": "^0.0.68",
    "@types/jest": "^26.0.22",
    "@types/uuid": "^8.3.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "~5.0.6",
    "@vue/test-utils": "^1",
    "@vue/vue2-jest": "^29.2.4",
    "babel-jest": "27.5.1",
    "babel-loader": "^8.2.2",
    "deepmerge": "^4.2.2",
    "jest": "27.5.1",
    "jest-extended": "2",
    "postcss": "^8.2.9",
    "sass": "~1.55.0",
    "sass-loader": "^13.0.2",
    "ts-jest": "27.1.5",
    "tslint": "^6.1.3",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "^5.2.2",
    "typescript-tslint-plugin": "^1.0.1",
    "vue-cli-plugin-vuetify": "^2.4.2",
    "vue-template-compiler": "2.7",
    "vuex-module-decorators": "^1.0.1",
    "workbox-webpack-plugin": "^6.5.4"
  }
}
