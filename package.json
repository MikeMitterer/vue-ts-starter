{
  "name": "@mmit/vuetsstarter",
  "version": "0.1.13",
  "private": true,
  "description": "VUE Starter-Template",
  "author": "Mike Mitterer",
  "scripts": {
    "start": "vue-cli-service serve --mode development --open",
    "start:prod": "vue-cli-service serve --mode production --open",
    "test:unit": "vue-cli-service test:unit tests/unit/.*",
    "tc": "tsc --noEmit",
    "lint": "vue-cli-service lint",
    "test": "yarn run test:unit",
    "test:integration": "vue-cli-service test:unit --runInBand --detectOpenHandles tests/integration/.*",
    "build": "vue-cli-service build --mode development",
    "build:prod": "vue-cli-service build --mode production",
    "deploy": "yarn version --new-version patch",
    "clean": "rm -rf lib dist bin doc && jest --clearCache"
  },
  "dependencies": {
    "@mmit/logging": "^0.1.5",
    "@mmit/styles": "^0.1.15",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.5",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.4.3",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "26.0.13",
    "@vue/cli-plugin-pwa": "~4.5.6",
    "@vue/cli-plugin-typescript": "^4.5.6",
    "@vue/cli-plugin-unit-jest": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "@vue/test-utils": "1.1.0",
    "babel-jest": "26.3.0",
    "jest": "26.4.2",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.2",
    "ts-jest": "26.3.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "^4.0.2",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.12",
    "vuex-module-decorators": "^0.17.0"
  },
  "keywords": [
    "typescript vue"
  ],
  "resolutions": {
    "@vue/cli-plugin-unit-jest/jest": "24",
    "@vue/cli-plugin-unit-jest/babel-jest": "24"
  }
}
