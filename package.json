{
  "name": "@mmit/vue-ts-starter",
  "version": "0.1.13",
  "private": true,
  "description": "VUE Starter-Template",
  "author": "Mike Mitterer",
  "scripts": {
    "pkg:version": "jq -r '.version' < package.json",
    "clean": "rm -rf lib dist bin && jest --clearCache",
    "update:browser": "browserslist --update-db",
    "download:certs": "rm -rf .ssl && cert2ssl.sh -d int.mikemitterer.at",
    "build": "vue-cli-service build --mode development",
    "test:unit": "vue-cli-service test:unit test/unit/.*",
    "lint": "vue-cli-service lint",
    "deploy": "yarn version --new-version patch",
    "build:prod": "vue-cli-service build --mode production",
    "start": "vue-cli-service serve --mode development --open",
    "start:prod": "vue-cli-service serve --mode production --open",
    "tc": "tsc --noEmit",
    "test": "yarn run test:unit",
    "test:integration": "vue-cli-service test:unit --runInBand --detectOpenHandles tests/integration/.*"
  },
  "dependencies": {
    "@mmit/logging": "^0.1.5",
    "@mmit/styles": "^0.1.21",
    "register-service-worker": "^1.7.1",
    "vue": "2.6.14",
    "vue-class-component": "^7.2.5",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@mdi/font": "^6.3.95",
    "@types/google.visualization": "^0.0.68",
    "@types/jest": "26",
    "@types/uuid": "^8.3.1",
    "@types/websocket": "^1.0.4",
    "@types/ws": "^7.4.7",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-typescript": "^4.5.14",
    "@vue/cli-plugin-unit-jest": "^4.5.14",
    "@vue/cli-service": "^4.5.14",
    "@vue/test-utils": "1.2.2",
    "babel-jest": "26",
    "babel-loader": "^8.2.4",
    "jest": "26",
    "jest-extended": "^2.0.0",
    "jest-transform-stub": "^2.0.0",
    "sass": "^1.32.8",
    "sass-loader": "10.1.1",
    "ts-jest": "26",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "^4.4.4",
    "vue-jest": "4",
    "vue-template-compiler": "2.6.14",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^1.0.1",
    "workbox-webpack-plugin": "^6.3.0"
  },
  "keywords": [
    "typescript vue"
  ]
}
